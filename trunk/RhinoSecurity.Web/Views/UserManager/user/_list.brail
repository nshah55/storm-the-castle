<table class='inline' style='width: 640px;'>
	<thead>
		<tr>
			<th>Name</th>
			<th>E-mail</th>
			<th>Mobile</th>
			<th>Disabled</th>
		</tr>
	</thead>
	<tbody>
<% for user in users: %>	
		<tr>
			<td>${user.Name}</td>
			<td>${user.Email}</td>
			<td>${user.Mobile}</td>
			<td>${user.Disabled.BoolToString()}</td>
		</tr>
<% end %>		
	</tbody>
	<tfoot>
		<tr>
			<th colspan='4'>&nbsp;</th>
		</tr>
	</tfoot>
</table>